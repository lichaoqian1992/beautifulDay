	<script>

	$(document).ready(function() {    

		    App.init(); 
		    infoset();
		    menuact("04_03_08");
		});
	
	function infoset(){
		
		var articlegoods=${articlegoods };
		
		var id=articlegoods.id;
		var article_id     =articlegoods.article_id;
		var goods_no       =articlegoods.goods_no ;
		var spec_ids       =articlegoods.spec_ids ;
		var spec_text      =articlegoods.spec_text ;
		var stock_quantity =articlegoods.stock_quantity ;
		var market_price   =articlegoods.market_price ;
		var sell_price     =articlegoods.sell_price ;
		

		$("#hiddenid").val(id);
		$("#article_id").val(article_id);
		$("#goods_no").val(goods_no);
		$("#spec_ids").val(spec_ids);
		$("#spec_text").val(spec_text);
		$("#stock_quantity").val(stock_quantity);
		$("#market_price").val(market_price);
		$("#sell_price").val(sell_price);


	};
	
	
	$("#subbutton").on("click",function(){

		$.ajax({
		type : "GET",
		url : "updArticleGoods",
		dataType : "json",
		contentType : "application/json; charset=utf-8",
		data : {
			id:$("#hiddenid").val(),
			article_id:$("#article_id").val(),
			goods_no:$("#goods_no").val(),
			spec_ids:$("#spec_ids").val(),
			spec_text:$("#spec_text").val(),
			stock_quantity:$("#stock_quantity").val(),
			market_price:$("#market_price").val(),
			sell_price:$("#sell_price").val()
				},
		success : function(data) {
			if(data.status==0){
				location.reload();
			}else{
				alert(修改失败);
			}
		}
				
		})
	});
	
	

	</script>

