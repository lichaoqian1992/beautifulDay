*{
	margin: 0;
	padding: 0;
}
.informant_center .mui-bar{
	box-shadow: none;
	border-bottom: 1px solid #EAEAEA;
}
.informant_center .mui-bar .mui-icon {
	color: #333;
	font-size: 35px;
	padding: 5px 0;
	
}
.informant_center .mui-bar .mui-title .i1{
	font-style: normal;
	font-size: 15px;
    padding-top: 2px;
    float: left;
}
.goods_news{
	background: #FFFFFF;
	padding: 0 10px;
	margin-bottom: 20px;
}
.goods_news h6{
	padding: 15px 0;
	margin: 0;
    font-size: 15px;
    color: #333333;
}
.goods_news_content:after{
	display: block;
	visibility: hidden;
	content: "";
	clear: both;
	height: 10px;
}
.goods_news_content img{
	width: 90px;
	height: 80px;
	padding: 0 10px 0 0;
	float: left;
	box-sizing: border-box;
}
.goods_news_content .goods_news_txt{
	float: left;
	/*padding: 0 0 0 10px;*/
	width: calc(100% - 90px);
}
.goods_news_content .goods_news_txt p{
	margin-bottom: 5px;
}
.goods_news_content .goods_news_txt p:nth-child(1){
	margin-bottom: 5px;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;

}
.informant_list{
	background-color: #FFFFFF;
	padding: 0 10px;
	margin-bottom: 20px;
}
.informant_list2{
	padding:0;
}
.informant_list h6{
	margin: 0;
	padding: 15px 0;
	font-size: 15px;
    color: #333333;
}
.informant_list h6 i{
	color: #f60;
	font-weight: bold;
	font-size: 20px;
	position: relative;
	top: 5px;
}
.informant_list ul:after{
	display: block;
	visibility: hidden;
	content: "";
	clear: both;
	
}
.informant_list ul li{
	display: inline-block;
	/* padding: 10px; */
	border: 1px solid #eaeaea;
	border-radius: 5px;
	/* float: left; */
	margin: 0 10px 10px 0;
	font-size: 14px;
	position: relative;
}
.informant_list2 ul li{
	display: block; 
	margin: 0 0 10px 0;
}
.myli{
	height: 31px;
}
.informant_list ul li button{
	border:none !important;
	padding:6px 10px !important;
}
.li_active{
	background: url(../images/select_icon1.png) no-repeat;
	background-position: bottom right;
	border: 1px solid #FF6600 !important;
}
.problem_type{
	background-color: #FFFFFF;
	padding: 0 10px;
	/*margin-bottom: 20px;*/
	border-bottom: 1px solid #EAEAEA;
}
.problem_type h6{
	margin: 0;
	padding: 15px 0 5px 0;
	font-size: 15px;
    color: #333333;
}
.problem_type h6 i{
	color: #f60;
	font-weight: bold;
	font-size: 20px;
	position: relative;
	top: 5px;
}
.problem_type textarea{
	margin: 0;
	padding: 5px;
	font-size: 14px;
	border: none;
		
}
.problem_type .word_total{
	text-align: right;
	margin-bottom: 5px;
	font-size: 14px;
}
.problem_type .word_total span:nth-child(2){
	font-size: 12px;
	color: #999999;
}
.upload{
	background: #FFFFFF;
	padding: 0 10px;
}
.upload h6{
	margin: 0;
    padding: 15px 0;
    font-size: 15px;
    color: #333333;
}
.upload h6 span:nth-child(2){
	color: #999;
	
}
.tip_text{
	padding:10px;
	font-size: 12px;
	
}

.mui-content-padded {
	margin-top: 30px;
}
.mui-btn{
	padding: 10px 0;
	color: #FFFFFF;
	background-color: #FF6600;
	border: 1px solid #FF6600;
}
.informant_center .mui-btn:active{
	background-color: #FF6600;
}

/*-----新增的css------*/
.feedback .image-list{
	padding: 10px 0 0 0;
}
.upload .upload_text{
	margin: 0;
	padding: 5px 0;
}
/*添加联系卖家*/
.goods_news h6 .a1{
	float: right;
	color: #000000;
}
.goods_news h6 .a1 i{
	background: url(../images/trade_icon.png) no-repeat;
	display: inline-block;
	width: 20px;
	height: 21px;
	vertical-align: middle;
	margin: -3px 5px 0 0;
}
/*申请人的css*/
.applicant_content .p1{
	padding: 10px;
	color: #333333;
	margin: 0;
}
.applicant_content .p1 .span1{
	color: #FF6600;
}
.applicant_content .applicant_person{
	background-color: #FFFFFF;
	padding: 0 10px;
	border-bottom: 1px solid #EAEAEA;
	font-size: 14px;
}

.applicant_content .applicant_person .span1{
	display: inline-block;
	width: 80px;
}
.applicant_content .applicant_person input{
	width: calc(100% - 80px);
	border: none;
	margin: 0;
	padding: 5px;
	font-size: 14px;
}

.applicant_content .applicant_tel{
	background-color: #FFFFFF;
	padding: 0 10px;
	font-size: 14px;
}
.applicant_content .applicant_tel .span1{
	width: 80px;
	display:inline-block;
}
.applicant_content .applicant_tel input{
	width: calc(100% - 80px);
	border: none;
	margin: 0;
	padding: 5px;
	font-size: 14px;
}
/*上传图片*/
#image-list .image-item:nth-child(6){
	display: none;
	
}

/*遮罩层*/
.success_mask{
	display: none;
}
.center_mask{
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: #666;
	opacity: 0.7;
	z-index: 999;
}
.mask_content{
	position: fixed;
	top: 50%;
	width: 80%;
	height: 174px;
	margin: -87px 0 0 10%;
	border-radius: 5px;
	font-size: 15px;
	color: #333;
	background-color: #fff;
	z-index: 9999;
	opacity: 10;
	text-align: center;
}

.mask_content img{
	margin: 20px 0 10px 0;
}
.mask_content .p1{
	font-size: 16px;
	color: #333333;
}
.mask_content .p2{
	margin: 0 3px;
	font-size: 12px;
}
/**/
.tip_mask{
	display: none;
}
.mask_content .mask_content_text{
	padding: 50px 0;
	border-bottom: 1px solid #EAEAEA;
}
.mask_content_btn .span1{
	display: inline-block;
	width: 50%;
	border-right: 1px solid #EAEAEA;
	margin: 15px 0;
}
.mask_content_btn .span2{
	display: inline-block;
	width: 50%;
	border-right: 1px solid #EAEAEA;
	margin: 15px 0;
	color: #FF6600;
}


.btn_tip_content3{
	display: none;
	width: 40%;
	position: fixed;
	top: 40%;
	left: 30%;
	padding: 10px 20px;
	border-radius: 5px;
	background: url(../../shopApp/images/report_center_icon5.png) no-repeat;
}