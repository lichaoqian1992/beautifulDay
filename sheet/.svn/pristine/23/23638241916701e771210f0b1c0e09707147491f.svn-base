*{
	margin: 0;
	padding: 0;
}
.informant .mui-bar{
	box-shadow: none;
	border-bottom: 1px solid #EAEAEA;
}
.informant .mui-bar .mui-icon {
	color: #333;
	font-size: 35px;
	padding: 5px 0;
	
}
.informant .mui-bar .mui-title .i1{
	font-style: normal;
	font-size: 15px;
    padding-top: 2px;
    float: left;
}

.informant_state{
	background-color: #3292ee;
	padding: 10px 0 2px 0;
}
.informant_state .title {
	text-align: center;
	margin: 0;
}

.informant_state .title i{
	background: url(../images/state1.png) no-repeat;
	display: inline-block;
	width: 20px;
	height: 20px;
}
.informant_state .title span{
	font-size: 17px;
	color: #FFFFFF;
	padding: 0 10px;
	font-weight: lighter;
	vertical-align: middle;
	
}
.informant_state .text{
	padding: 0 10px;
	color: #FFFFFF;
}
.informant_state .text .text_p1{
	color: #FFFFFF;
	font-size: 17px;
}
.informant_state .text .text_p2{
	color: #FFFFFF;
}
.informant_state .informant_progress{
	width: 96%;
	margin: 5px auto;
	background-color: #FFFFFF;
	height: 150%;
	border-radius: 5px;
}
ul,li{
	list-style: none;
	
}
.informant_progress .item{
	height: 70px;
    position: relative;
    padding: 10px 0 0 0;
}
.informant_progress .item:after{
	display: block;
	visibility: hidden;
	content: "";
	clear: both;
}
.informant_progress .item .item_span1{
	display: inline-block;
	width: 10px;
	height: 10px;
	background-color: #3292ee;
	border-radius: 50%;
	margin: 0 5px;
	position: relative;
	top: 8px;
	float: left;
}
.informant_progress .item .item_p2{
	display: inline-block;
	width: 150px;
	font-size: 14px;
	text-align: center;
	float: left;
	margin: 0;
}
@media  (max-width: 321px) {
.informant_progress .item .item_p2{
		width: 120px;
	}
	
}
.informant_progress .item .item_span3{
	background: url(../images/state1_right.png) no-repeat;
	width: 30px;
	height: 16px;
	display: inline-block;
    vertical-align: top;
    /*padding: 20px 0;*/
    margin: 20px 0;
    float: left;
}
.informant_progress .item:nth-child(1):before{
	display: inline-block;
	width: 1px;
	height: 95px;
	background-color: #3292ee;
	content: "";
	position: absolute;
	top: 20px;
	left: 10px;
}
.informant_progress .item:nth-child(2){
	margin: 20px 0 0 0;
}

.list_second{
	background-color: #FFFFFF;
	padding: 10px;
	margin: 20px 0 0 0;
}
.list_second .p_title{
	color: #000000;
}
.list_third{
	background-color: #FFFFFF;
	padding: 10px;
	margin: 20px 0 0 0;
}
.list_third .third_title{
	padding: 0 0 10px 0;
	border-bottom: 1px solid #EAEAEA;
	margin:0 0 10px 0;
	font-size: 14px;
	
}
.list_third .third_title p .span1{
	/*color: #8f8f94;*/
	color: #000000;
}
.list_third .third_title p .span3{
	color: #FF6600;
}
.list_third .third_title p:last-child{
	margin: 0;
}
.list_third .third_problem .span1{
	color: #FF6600;
}
.list_third .third_problem .span2{
	color: #000000;
	font-size: 14px;
}
.list_third .third_problem .span3{
	/*color: #000000;*/
	color: #8f8f94;
	font-size: 14px;
	float: right;
}
.list_third .third_problem .img1{
    width: 70px;
}
.list_fourth_title{
	margin: 10px;
	font-size: 14px;
}
.list_fourth{
	background: #FFFFFF;
	padding: 10px;
}
.list_fourth .fourth_item1{
	background: url(../images/seller_img.png) no-repeat;
	width: 100%;
	height: 27px;
	color: #FFFFFF;
	line-height: 27px;
	padding: 0 10px;
	font-size: 14px;

}
.list_fourth .fourth_item1:after{
     display: block;
     content: "";
     visibility: hidden;
     clear: both;
}

.list_fourth .fourth_item2:after{
     display: block;
     content: "";
     visibility: hidden;
     clear: both;
}
.list_fourth .fourth_item3{
	background: url(../images/user_img.png) no-repeat;
	position:relative;

}
.item2_left{
	width: 20px;
	height: 124px;
	float: left;
	position: relative;
}
 .item2_left .item2_left_point{
	width: 12px;
	height: 12px;
	border-radius: 50%;
	background-color: #37a1ff;
	float: left;
	position: absolute;
	top: 32px;
	z-index: 2;

	}
 .item2_left .item2_left_point2{
 	background-color: #f9af48;
 }
 .item2_left .item2_left_line{
	width: 1px;
	height: 124px;
	background-color: #f0f0f0;
	position: absolute;
	left: 5px;

}

.item2_right{
	width: calc(100% - 20px);
	float: right;
}
 .item2_right .item2_time{
	text-align: center;
	margin: 5px 0;
	color: #8f8f94;
}
 .item2_right .item2_text{
	background-color: #f5f5f5;
	padding: 10px;
	border-radius: 5px;
	color: #000000;
}

.item2_right .item2_bac2{
	background-color: #fff7eb;
}
.item2_right .item2_text img{
	width: 60px;
	height: 60px;
}



.mui-bar-nav~.mui-content {
	padding-bottom: 44px;
}


.footer{
	position: fixed;
	bottom: 0;
	height: 44px;
	width: 100%;
}
.footer_a{
	display: inline-block;
	width: 50%;
	line-height: 44px;
	text-align: center;
	float: left;
}
.footer_a1{
	color: #000000;
	background-color: #FFFFFF;
}
.footer_a2{
	background-color: #FF6600;
	color: #FFFFFF;
}


