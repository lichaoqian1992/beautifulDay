$(function(){
  var hight = $(".list_fourth").height();
  console.log(hight);
  var hight2 = hight-40;
  $(".fourth_item2 .item2_left .item2_left_line").css("height",hight2);
})
/**
 * 点击已解决
 * @param obj
 */
function deal(obj){
	alert(obj);
	$("#sheet_no").val(obj);
	$(".tip_mask").show();
}
function cancel(){
	$(".tip_mask").hide();
}
function makeSure(){
	var sheet_no = $("#sheet_no").val();
	alert(sheet_no);
	$.ajax({
		type:"get",
		url:"/sheet/buyersApp/updateStatus",
		dataType:"json",
		data:{
			status:6,
			sheet_id:sheet_no
		},success:function(data){
			if(data.message == "SUCCESS"){
				
				window.location.href="/sheet/buyersApp/myInfo?sponsoId=1&codeReport=01-02-03";
				$(".tip_mask").hide();
			}
		}
			
	});
}