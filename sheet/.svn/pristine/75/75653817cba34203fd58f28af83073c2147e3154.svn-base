*{
	margin: 0;
	padding: 0;
}
.trade_dispute_list .mui-bar{
	box-shadow: none;
	border-bottom: 1px solid #EAEAEA;
}
.trade_dispute_list .mui-bar .mui-icon {
	color: #333;
	font-size: 35px;
	padding: 5px 0;
	
}
.trade_dispute_list .mui-bar .mui-title .i1{
	font-style: normal;
	font-size: 15px;
    padding-top: 2px;
    float: left;
}

.trade_dispute_state{
	width: 100%;
	position: fixed;
	top: 44px;
	height: 40px;
}
.trade_dispute_state .state_apply1{
	width: 50%;
	height: 40px;
	float: left;
	text-align: center;
	line-height: 40px;
	background-color: #FFFFFF;
	color: #8F8F94;
	font-size: 14px;
}
.trade_dispute_state .state_apply1 .span_active{
	display: inline-block;
	color: #FF6600;
	border-bottom: 2px solid #FF6600;
	
	
}

.trade_dispute_list .mui-content{
	padding-top: 85px;
}
ul,li{
	list-style: none;
}

.state1_ul{
	/*padding: 0 10px;*/
}
.state1_li:nth-child(1){
	margin-top: 5px;
}
.state1_li{
	margin-bottom: 20px;
}
.state1_li .item_top{
	padding: 10px;
	background-color: #FFFFFF;
	font-size: 14px;
}
.state1_li .item_top .span1{
	color: #8F8F94;
}
.state1_li .item_top .a1{
	float: right;
	color: #FF6600;
}
.state1_li .item_content{
   background-color:#fafafa ;
   padding: 10px;
}
.state1_li .item_content img{
	width: 90px;
	padding-right: 10px;
	box-sizing: border-box;
}
.state1_li .item_content .text{
	width: calc(100% - 90px);
	float: right;
}
.state1_li .item_content .text .p1{
	margin: 0;
	overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    /*height:50px;*/
}
.state1_li .item_bottom{
	padding: 10px;
	background-color: #FFFFFF;
	font-size: 14px;
}
.state1_li .item_bottom .span1{
	color: #8F8F94;
}
.state1_li .item_bottom .a1{
	float: right;
	color: #000000;
	border: 1px solid #c6c6c6;
	padding: 2px 5px;
	border-radius: 4px;
	margin-top: -4px;
}
