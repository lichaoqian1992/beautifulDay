#@pclayout()
#define main()
<!--描述：页面内容部分-->
		<div class="kf_content_box">
			<div class="kf_bg_box">
				<div class="kf_nav">
					<a class="kf_nav_home kf_nav_item" href="/sheet/home">首页</a>
					<a class="kf_nav_item">基础设置</a>
					<span class="kf_nav_item kf_nav_item_last">工单类型</span>
				</div>
				<div class="kf_type_block">
					<div class="kf_form_title">
						<span class="kf_form_content kf_icon kf_icon_shop">举报商家</span>
					</div>
					<div class="kf_search_box">
						<div class="kf_group">
							<div class="kf_group_content">
								<button type="button" class="kf_btn kf_add_consult">新 增</button>
							</div>
						</div>
						<div class="kf_group">
							<div class="kf_group_content">
								<button type="button" class="kf_btn" onclick="back()">返 回</button>
							</div>
						</div>
					</div>
					<div class="kf_table_box">
						<table class="kf_table">
							<thead>
								<tr>
									<th>序号</th>
									<th>排序</th>
									<th>类型名称</th>
									<th>操作</th>
								</tr>
							</thead>
							<tbody>
								#for(x : info)
								<tr>
									<td>#(for.index+1)</td>
									<td style="display: none;" id="code_id">#(x.id)</td>
									<td id="x_sortId">#(x.sort_id)</td>
									<td id="x_title">#(x.title)</td>
									<td>
										<a class="kf_table_a_edit" href="#">编辑</a>
										#if(x.status == '1')
											<a class="kf_table_a_forbidden" href="#">禁用</a>
										#elseif(x.status == '0')
											<a class="kf_table_a_enabled" href="#">启用</a>
										#end
									</td>
								</tr>
								#end
							</tbody>
						</table>
						<div class="kf_table_page">
							<div class="kf_page_content kf_float_right ">
								<span class="kf_page_text">每页<i>15</i>个，共<i>1</i>页</span>
								<ul class="kf_page_size">
									<li>
										<a class="kf_page_prev"></a>
									</li>
									<li class="active">
										<a>1</a>
									</li>
									<li>
										<a class="kf_page_next"></a>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>

		</div>
		<!--描述：页面内容部分结束-->
		
		<!--描述：弹出窗-->
		<div class="kf_mode_box ">
			<div class="kf_mode_shield"></div>
			<div class="kf_mode_content kf_mode_add">
				<div class="kf_mode_header">
					<label class="kf_mode_title">新增举报类型</label>
				</div>
				<div class="kf_mode_body">
					<div class="kf_group">
						<span class="kf_group_title">类型名称：</span>
						<div class="kf_group_content">
							<input class="kf_form_text" type="text" placeholder="类型名称" istype="string" style="width: 370px;">
							<div class="kf_add_consult_tips"></div>
						</div>
					</div>
					
					<div class="kf_group">
						<span class="kf_group_title">排&emsp;&emsp;序：</span>
						<div class="kf_group_content">
							<input class="kf_form_text" type="text" istype="number" placeholder="数字越小排位越靠前" style="width: 370px;">
							<div class="kf_add_consult_tips"></div>
						</div>
					</div>
				</div>
				<div class="kf_mode_footer">
					<button class="kf_btn kf_btn_grey kf_mode_btn_close" type="button">取 消</button>
					<button class="kf_btn kf_mode_btn_add_ok" type="button">确 定</button>
				</div>
			</div>
			<div class="kf_mode_content kf_mode_edit">
				<div class="kf_mode_header">
					<label class="kf_mode_title">编辑举报类型</label>
				</div>
				<div class="kf_mode_body">
					<div class="kf_group">
						<span class="kf_group_title">类型名称：</span>
						<div class="kf_group_content">
							<input class="kf_form_text" type="text" value="服务态度" placeholder="类型名称" istype="string" style="width: 370px;">
							<div class="kf_add_consult_tips"></div>
						</div>
					</div>
					
					<div class="kf_group">
						<span class="kf_group_title">排&emsp;&emsp;序：</span>
						<div class="kf_group_content">
							<input class="kf_form_text" value="2" type="text" istype="number" placeholder="数字越小排位越靠前" style="width: 370px;">
							<div class="kf_add_consult_tips"></div>
						</div>
					</div>
				</div>
				<div class="kf_mode_footer">
					<button class="kf_btn kf_btn_grey kf_mode_btn_close" type="button">取 消</button>
					<button class="kf_btn kf_mode_btn_add_ok" type="button">确 定</button>
				</div>
			</div>
			<div class="kf_mode_content kf_mode_affirm">
				<div class="kf_mode_header">
					<label class="kf_mode_title">请确认操作</label>
				</div>
				<div class="kf_mode_body kf_display_table">
					<div class="kf_display_table_cell">
						<label>温馨提示</label>
						<p>确认<i class="kf_color_danger">禁用</i>该类型嘛？</p>
					</div>
				</div>
				<div class="kf_mode_footer">
					<button class="kf_btn kf_btn_grey kf_mode_btn_close" type="button">取 消</button>
					<button class="kf_btn kf_mode_btn_ok" type="button">确 定</button>
				</div>
			</div>
		</div>
		<!--描述：弹出窗结束-->
		<script type="text/javascript" src="js/jquery-1.11.0.js"></script>
		<script type="text/javascript">
			$(function(){
				$(".kf_add_consult").click(function(){
					$(".kf_mode_add, .kf_mode_shield").show();
				});
				$(".kf_table").on("click",".kf_table_a_edit",function(){
					$(".kf_mode_edit, .kf_mode_shield").show();
				});
				
				$(".kf_table").on("click",".kf_table_a_forbidden",function(){
					$(".kf_mode_affirm, .kf_mode_shield").show();
				});
				$(".kf_mode_btn_close").click(function(){
					$(".kf_mode_content, .kf_mode_shield").hide();
				});
				$(".kf_mode_btn_add_ok").click(function(){
					var $input=$(this).parents(".kf_mode_add").find("input");
					$input.each(function(){
						var f=$(this).val();
						if(f==""){
							$(this).siblings(".kf_add_consult_tips").text("请输入信息");
							return false;
						}else{
							$(this).siblings(".kf_add_consult_tips").text("");
						}
					});
				});
			});
			//返回上一级
			function back(){
				window.location.href="/sheet/base/findJB";
			}
		</script>
#end