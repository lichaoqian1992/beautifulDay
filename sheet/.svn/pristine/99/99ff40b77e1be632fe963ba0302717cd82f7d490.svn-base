/*进度条*/
.details_status_progress {
    padding: 30px 0;
    overflow: hidden;
    zoom: 1;
    border-bottom: 1px solid #d2d2d2;
}
.status_info{
	padding-top: 30px;
}
.details_status_progress li {
    position: relative;
    height: 100px;
    width: 190px;
    float: left;
}
.details_status_progress li {
    position: relative;
    height: 100px;
    width: 190px;
    float: left;
}
.details_status_progress li:first-child {
    width: 90px;
}
.details_status_progress span {
    position: absolute;
    text-align: center;
}
.details_status_progress .active .details_status_progress_name {
    color: #087dd1;
}
.details_status_progress_name {
    width: 70px;
    top: 10px;
    right: -2px;
    font-size: 16px;
    color:#666;
}
.details_status_progress_time {
    width: 130px;
    bottom: 12px;
    right: -35px;
    font-size: 12px;
    color:#526069;
}
.details_status_progress li:before {
    content: "";
    position: absolute;
    top: 50px;
    height: 1px;
    width: 120px;
    border-bottom: 1px dashed #ccc;
}
.details_status_progress li:first-child:before {
    height: 0;
    width: 0;
    border-bottom: 0;
}
.details_status_progress li:after {
    content: "";
    position: absolute;
    top: 38px;
    right: 20px;
    display: block;
    height: 25px;
    width: 25px;
    background: url(../images/icon/list_img_n.png) no-repeat center;
}
.details_status_progress li.active:after {
    background: url(../images/icon/list_img.png) no-repeat center;
}


