            <div class="kf_group">
                <label class="kf_group_title">卖家昵称：</label>
                <div class="kf_group_content">#(remarkRecordAll.nick_name)</div>
            </div>
            <div class="kf_group">
                <label class="kf_group_title">发货地址：</label>
                <div class="kf_group_content">#(ShipAddress.address)</div>
            </div>
            <div class="kf_group">
                <label class="kf_group_title">收货地址：</label>
                <div class="kf_group_content">#(BuyersInfo.address)</div>
            </div>
            <div class="kf_group">
                <label class="kf_group_title"></label>
                <div class="kf_group_content">
                	#for(x : BuyersList)
                		<div class="kf_logistics_goods">
	                        <img src="#(x.img_url)">
	                        <label><i class="kf_color_danger">#(x.real_price)</i>x#(x.quantity)</label>
                    	</div>
					#end
                </div>
            </div>
        </div>
    </div>
    #end
</div>
<script type="text/javascript">
//下面用于多图片上传预览功能

function setImagePreviews(avalue) {
    var docObj = document.getElementById("doc");

    var dd = document.getElementById("dd");

    dd.innerHTML = "";

    var fileList = docObj.files;

    for (var i = 0; i < fileList.length; i++) {            



        dd.innerHTML += "<li class='kf_upload_item'><span class='kf_upload_box'><img id='img" + i + "'  /></span></li>";

        var imgObjPreview = document.getElementById("img"+i); 

        if (docObj.files && docObj.files[i]) {

            //火狐下，直接设img属性

            imgObjPreview.style.display = 'block';

            imgObjPreview.style.width = '120px';

            imgObjPreview.style.height = '120px';

            //imgObjPreview.src = docObj.files[0].getAsDataURL();

            //火狐7以上版本不能用上面的getAsDataURL()方式获取，需要一下方式

            imgObjPreview.src = window.URL.createObjectURL(docObj.files[i]);

        }

        else {

            //IE下，使用滤镜

            docObj.select();

            var imgSrc = document.selection.createRange().text;

            alert(imgSrc)

            var localImagId = document.getElementById("img" + i);

            //必须设置初始大小

            localImagId.style.width = "150px";

            localImagId.style.height = "180px";

            //图片异常的捕捉，防止用户修改后缀来伪造图片

            try {

                localImagId.style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod=scale)";

                localImagId.filters.item("DXImageTransform.Microsoft.AlphaImageLoader").src = imgSrc;

            }

            catch (e) {

                alert("您上传的图片格式不正确，请重新选择!");

                return false;

            }

            imgObjPreview.style.display = 'none';

            document.selection.empty();
            
        }
    }  
	$(".shuzi").html(fileList.length);
	if(fileList.length>=5){
		$("#dd").next().hide();
	}

    return true;
}

    $(function(){
    	/*
    	 *  弹出框
    	 */
    	//关闭弹出框
    	$(".kf_mode_close, .kf_mode_btn_close").click(function() {
    		$(".kf_mode_content, .kf_mode_shield").hide();
    	});
    	//弹出框单击事件
    	$(".kf_mode_btn_ok").click(function() {
    		$(this).parents(".kf_mode_content").hide();
    		if($(".kf_mode_shield").attr("istype") == "zhix") {
    			$(".kf_mode_tips").find(".kf_mode_tips_box").removeClass("kf_mode_tips_error");
    			$(".kf_mode_tips").show().find(".kf_mode_tips_box").text("执行成功");
    			$(".kf_form_btn_tuis").removeClass("kf_btn_disabled kf_btn_grey_main");
    			$(".kf_form_penalize_execute_btn_a").addClass("kf_form_penalize_execute_btn_a_no");
    			interval = setInterval(function() {
        			$(".kf_mode_content, .kf_mode_shield").hide();
        			clearInterval(interval);
        		}, 1000);
    		} else if($(".kf_mode_shield").attr("istype") == "tuisong") {
    			$(".kf_mode_tips").find(".kf_mode_tips_box").removeClass("kf_mode_tips_error");
    			$(".kf_mode_tips").show().find(".kf_mode_tips_box").text("推送成功");
    			$(".kf_form_btn_tuis").addClass("kf_btn_disabled kf_btn_grey_main");
    			interval = setInterval(function() {
        			$(".kf_mode_content, .kf_mode_shield").hide();
        			clearInterval(interval);
        		}, 1000);
    		} else {
    			submitChuFa();
    			$(".kf_mode_tips").find(".kf_mode_tips_box").removeClass("kf_mode_tips_error");
    			$(".kf_mode_wait, .kf_mode_shield").show();
    			$("#chufa").submit();
    			//$(".kf_mode_tips").show().find(".kf_mode_tips_box").text("保存成功");
    		}
    	});
    	
        //执行处罚修改单击事件
        $(".kf_form_penalize_edit").click(function(){
            $(".kf_mode_arbitration, .kf_mode_shield").show();
        });


        //有无处罚单击事件
        $(".kf_form_title input[type='checkbox']").click(function(){
            if($(this).prop("checked")){
                $(this).parent().addClass("active");
                $(".kf_form_penalize>.kf_form_penalize_btn").addClass("kf_hide");
                $('input[name="Ishandle"]').val("false");
            }else{
                $(this).parent().removeClass("active");
                $(".kf_form_penalize>.kf_form_penalize_btn").removeClass("kf_hide");
                $('input[name="Ishandle"]').val("true");
            }
        });
        
      	//受理不受理单击事件
		$(".kf_form_result input[type='radio']").click(function() {
			$(this).parent().addClass("active").siblings().removeClass("active");
		});

        //页面确定单击事件
        $(".kf_form_ok").click(function(){
        	if($('textarea[name="explain"]').val()==""){
       			$(".kf_mode_tips").find(".kf_mode_tips_box").addClass("kf_mode_tips_error");
   				$(".kf_mode_tips").show().find(".kf_mode_tips_box").text("仲裁描述未填写！");
   				interval = setInterval(function() {
   	    			$(".kf_mode_content, .kf_mode_shield").hide();
   	    			clearInterval(interval);
   	    		}, 1000);
       			return;
        	}
        	if($(".aClasscode").val()!="false"){
        		var status=0;
            	$(".chufaalso .kf_form_penalize_item").each(function(){
            		if($(this).find("label").hasClass("active")){
            			status=1;
            		}
            	})
            	if($(".kf_checkbox_orange").hasClass("active")){
        			status=1;
        		}
            	if(status==0){
        			$(".kf_mode_tips").find(".kf_mode_tips_box").addClass("kf_mode_tips_error");
    				$(".kf_mode_tips").show().find(".kf_mode_tips_box").text("请选择处罚或勾选无处罚！");
    				interval = setInterval(function() {
    	    			$(".kf_mode_content, .kf_mode_shield").hide();
    	    			clearInterval(interval);
    	    		}, 1000);
        			return;
        		}
        	}
        	
            $(".kf_mode_affirm, .kf_mode_shield").show();
        });


        //弹出框单击事件
        $(".kf_mode_arbitration_btn_ok").click(function(){
        	//获得有输入框的选中个数
            var che=$(".kf_mode_arbitration .kf_checkbox input[type='checkbox']:checked").siblings(".kf_form_text");
            var index=0;
            che.each(function(){
                var $val=$.trim(che.val());
                if($val!=""){
                    index++;
                }
            });

            if(che.length==index){
                $(this).parents(".kf_mode_content").hide();
                $(".kf_mode_tips").show();
                $(".kf_mode_content, .kf_mode_shield").hide();
                $(".kf_form_penalize_content").removeClass("kf_hide").siblings().addClass("kf_hide");
                $(".kf_form_penalize_mode_tips").addClass("kf_hide");
                $(".chufaalso").html($(".chufa").html());
            	$(".chufashopalso").html($(".chufashop").html());
            	$(".chufaalso .sum:eq(0)").val($(".chufa .sum:eq(0)").val());
            	$(".chufaalso .sum:eq(1)").val($(".chufa .sum:eq(1)").val());
            	$(".chufaalso .sum:eq(2)").val($(".chufa .sum:eq(2)").val());
            	$(".chufaalso .Bond select").val($(".chufa .Bond select").val());
            	$(".chufaalso .Bond select").attr("disabled","disabled");
            	$(".chufaalso .sum").attr("readonly","readonly");
            }else{
                $(".kf_form_penalize_mode_tips").removeClass("kf_hide");
            }

        });

        //仲裁弹出框显示
        $(".kf_form_penalize_button").click(function(){

            $(".kf_mode_arbitration, .kf_mode_shield").show();
            var h=$(".kf_mode_arbitration").height();
            $(".kf_mode_arbitration").css("margin-top",-h/2+"px");
        });
        //仲裁弹出框选择
        $(".kf_form_penalize_item input[type='checkbox']").click(function(){

            //判断强制下架
            if($(this).hasClass("goods_list_show") && $(this).prop("checked")){
                $(this).parents(".kf_form_penalize_list").siblings(".kf_form_penalize_goods_list").removeClass("kf_hide");
                var h=$(".kf_mode_arbitration").height();
                $(".kf_mode_arbitration").css("margin-top",-h/2+"px");
            }else if($(this).hasClass("goods_list_show") && !$(this).prop("checked")){
                $(this).parents(".kf_form_penalize_list").siblings(".kf_form_penalize_goods_list").addClass("kf_hide");
                var h=$(".kf_mode_arbitration").height();
                $(".kf_mode_arbitration").css("margin-top",-h/2+"px");
                $(".shop").removeClass("active");
            }

            //判断是否选中
            if($(this).prop("checked")){
            	$(this).parent().find(".punish").val(1);
                $(this).parent().addClass("active");

            }else{
            	$(this).parent().find(".punish").val("");
                $(this).parent().removeClass("active");
            }
        });
    });
    
    
    function submitChuFa(){
    	
    	if($(".chufaalso .kf_form_penalize_item:eq(0) label").hasClass("active")){
    		$('input[name="reputation"]').val($(".chufaalso .sum:eq(0)").val());
    	}else{
    		$('input[name="reputation"]').val("false");
    	}
    	
		if($(".chufaalso .kf_form_penalize_item:eq(1) label").hasClass("active")){
			if($(".codeTypeOne").val()=="02"){
				$('input[name="bond"]').val($(".chufaalso .Bond select").val()+","+$(".chufaalso .sum:eq(1)").val());
			}else{
				$('input[name="bond"]').val($(".chufaalso .sum:eq(1)").val());
			}
		}else{
			$('input[name="bond"]').val("false");
		}
		
		if($(".chufaalso .kf_form_penalize_item:eq(2) label").hasClass("active")){
			$('input[name="compensate"]').val($(".chufaalso .sum:eq(2)").val());
		}else{
			$('input[name="compensate"]').val("false");
		}
		
		/*if($(".chufaalso .kf_form_penalize_item:eq(3) label").hasClass("active")){
			$('input[name="closedShop"]').val($(".shopIdalso").val());
		}else{
			$('input[name="closedShop"]').val("false");
		}*/
		
		if($(".chufaalso .kf_form_penalize_item:eq(3) label").hasClass("active")){
			$('input[name="frozen"]').val($(".shopUserId").val());
		}else{
			$('input[name="frozen"]').val("false");
		}
		var stu="";
		if($(".chufaalso .kf_form_penalize_item:eq(4) label").hasClass("active")){
			$(".chufashopalso .kf_form_penalize_item").each(function(){
				if($(this).find(".shop").hasClass("active")){
					stu+=$(this).find(".shopId").val()+",";
				}
			})
			if(stu==""){
				$('input[name="shopId"]').val("false");
			}else{
				$('input[name="shopId"]').val(stu);
			}
		}else{
			$('input[name="shopId"]').val("false");
		}
		
		$(".kf_radio").each(function(){
			if($(this).hasClass("active")){
				if($(this).find("input").val()==1){
					$('input[name="evaluate"]').val($(".pingbi").val());
					$('input[name="pingjia"]').val("true");
				}
			}
		})
		
    }
</script>