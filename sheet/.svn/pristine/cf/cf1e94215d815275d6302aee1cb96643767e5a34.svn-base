<div class="kf_mode_box ">
    <div class="kf_mode_shield"></div>
    <div class="kf_mode_content kf_mode_affirm">
        <div class="kf_mode_header">
            <label class="kf_mode_title">请确认操作</label>
        </div>
        <div class="kf_mode_body kf_display_table">
            <div class="kf_display_table_cell">
                <label>温馨提示</label>
                <p>请仔细审查工单，确定仲裁合理。</p>
            </div>
        </div>
        <div class="kf_mode_footer">
            <button class="kf_btn kf_btn_grey kf_mode_btn_close" type="button">取 消</button>
            <button class="kf_btn kf_mode_btn_ok" isclass=".kf_mode_affirm" type="button">确 定</button>
        </div>
    </div>
    <div class="kf_mode_content kf_mode_arbitration">
        <div class="kf_mode_header">
            <label class="kf_mode_title">仲裁结果录入</label>
            <a class="kf_mode_close"></a>
        </div>
        <div class="kf_mode_body">
            <div class="kf_form_penalize_list chufa">
                <div class="kf_form_penalize_item">
                    <label class="kf_checkbox">
                        <input type="checkbox">扣信誉值：<input class="kf_form_text sum"  type="number" placeholder="请录入数字"/>分
                    </label>
                </div>
                <div class="kf_form_penalize_item">
                    <label class="kf_checkbox">
                        <input type="checkbox">扣保证金：<input class="kf_form_text sum"  type="number" placeholder="请录入数字"/>元
                        <label class="Bond">
	                        #if(WorkNumber.OneCode=="02")
		                       	取保业务类型:
		                        <select class="kf_form_text">
		                        	#for(x : articleListBond)
										<option value="#(x.id)">#(x.name)</option>
									#end
		                        </select>
	                        #end
                        </label>
                    </label>
                </div>
                <div class="kf_form_penalize_item">
                    <label class="kf_checkbox">
                        <input type="checkbox">
                        赔  偿：<input class="kf_form_text sum" type="number" placeholder="请录入数字"/>元<span>（从待结算的钱里扣除）</span>
                    </label>
                </div>
                <!-- <div class="kf_form_penalize_item">
                    <label class="kf_checkbox">
                        <input type="checkbox"><i class="kf_color_danger">强制关店</i>
                    </label>
                </div>  -->
                <div class="kf_form_penalize_item">
                    <label class="kf_checkbox">
                        <input type="checkbox"><i class="kf_color_danger">冻结店铺财务账号（不能提现）</i>
                    </label>
                </div>
                 #if(articleList.size()>0)
                <div class="kf_form_penalize_item">
                    <label class="kf_checkbox">
                        <input type="checkbox" class="goods_list_show">强制下架
                    </label>
                </div>
                #end
            </div>
            
			#if(articleList.size()>0)
            <div class="kf_form_penalize_goods_list kf_hide chufashop" id="goods_list">
                <label>已选择商品</label>
                #for(x : articleList)
                <div class="kf_form_penalize_item">
                    <label class="kf_checkbox shop">
                        <input type="checkbox">
                        <input type="hidden" value="#(x.id)" class="shopId"/>
                        <img src="#(x.img_url)">
                      #(x.title)
                    </label>
                </div>
                #end
            </div>
             #end
            <div class="kf_form_penalize_mode_tips kf_hide" >你有勾选的处罚项未填写</div>
        </div>
        <div class="kf_mode_footer">
            <button class="kf_btn kf_btn_grey kf_mode_btn_close" type="button">取 消</button>
            <button class="kf_btn kf_mode_arbitration_btn_ok" isclass=".kf_mode_arbitration"type="button">确 定</button>
        </div>
    </div>
    <div class="kf_mode_content kf_mode_tips">
        <div class="kf_mode_tips_box">
            保存成功
        </div>
    </div>
 	<div class="kf_mode_content kf_mode_wait">
		<div class="kf_mode_tips_wait">
			请稍等...
		</div>
	</div>
    #if(remarkRecordAll!=null)
