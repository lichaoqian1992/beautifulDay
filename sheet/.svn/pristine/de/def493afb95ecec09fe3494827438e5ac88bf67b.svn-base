<!doctype html>
<html>

	<head>
		<meta charset="UTF-8">
		<title>纠纷管理</title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link rel="stylesheet" type="text/css" href="#(ctx)/static/shopApp/css/mui.min.css" />
		<link rel="stylesheet" type="text/css" href="#(ctx)/static/shopApp/css/dispute_manage.css" />
	</head>

	<body class="dispute_manage">
		<header class="mui-bar mui-bar-nav">
			<a class="mui-action-back mui-icon mui-icon-left-nav mui-pull-left"> </a>
			<h1 class="mui-title">纠纷管理</h1>
			<a class="a1" href="javascript:void(0);"></a>
		</header>
		<div class="mui-content">
			<div class="content">
				<ul class="header_title">
					<li><span class="span1 span_active">全部</span></li>
					<li><span class="span2">处理中</span></li>
					<li><span class="span3">已完成</span></li>
				</ul>
				<ul class="list1">
					#for(all:disputeList)
					<li class="item" onclick="finddispute(this)">
						<div class="item_left">
							<p class="p1"><span class="span1">纠纷单号:</span><span class="span2">#(all.sheet_no)</span></p>
							<p class="p2"><span class="span1">订单编号:</span><span class="span2">#(all.order_no)</span></p>
							<div class="div1">
								<img class="img1" src="#(all.img_url)" />
								<p class="div1_p1">#(all.goods_title)</p>
							</div>
							#if(all.status=="3")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon4.png" />
							#end
							#if(all.status=="4")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon4.png" />
							#end
							#if(all.status=="5")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon5.png" />
							#end
						</div>
					</li>
					<input type="hidden" value="#(all.id)"/>
					#end
				</ul>

				<ul class="list2">
					#for(d:ddisputeList)
					<li class="item" onclick="finddispute(this)">
						<div class="item_left">
							<p class="p1"><span class="span1">纠纷单号:</span><span class="span2">#(d.sheet_no)</span></p>
							<p class="p2"><span class="span1">订单编号:</span><span class="span2">#(d.order_no)</span></p>
							<div class="div1">
								<img class="img1" src="#(d.img_url)" />
								<p class="div1_p1">#(d.goods_title)</p>
							</div>
							#if(d.status=="3")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon4.png" />
							#end
							#if(d.status=="4")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon4.png" />
							#end
							#if(d.status=="5")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon5.png" />
							#end
						</div>
					</li>
					<input type="hidden" value="#(d.id)"/>
					#end
				</ul>

				<ul class="list3">
					#for(w:wdisputeList)
					<li class="item" onclick="finddispute(this)">
						<div class="item_left">
							<p class="p1"><span class="span1">纠纷单号:</span><span class="span2">#(w.sheet_no)</span></p>
							<p class="p2"><span class="span1">订单编号:</span><span class="span2">#(w.order_no)</span></p>
							<div class="div1">
								<img class="img1" src="#(w.img_url)" />
								<p class="div1_p1">#(w.goods_title)</p>
							</div>
							#if(w.status=="3")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon4.png" />
							#end
							#if(w.status=="4")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon4.png" />
							#end
							#if(w.status=="5")
							<img class="item_left_img1" src="#(ctx)/static/shopApp/images/report_progress_icon5.png" />
							#end
						</div>
					</li>
					<input type="hidden" value="#(w.id)"/>
					#end
				</ul>
			</div>

		</div>

		<script type="text/javascript" src="#(ctx)/static/shopApp/js/jquery-1.11.0.js"></script>
		<script type="text/javascript" src="#(ctx)/static/shopApp/js/mui.min.js"></script>
		<script type="text/javascript" src="#(ctx)/static/shopApp/js/dispute_manage.js"></script>
		<script type="text/javascript" src="#(ctx)/static/shopApp/js/dispute/dispute_manage.js"></script>
	</body>

</html>