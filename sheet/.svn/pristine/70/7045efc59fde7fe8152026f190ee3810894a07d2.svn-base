*{
	margin: 0;
	padding: 0;
}
.search .mui-bar{
	box-shadow: none;
	border-bottom: 1px solid #EAEAEA;
}
.search .mui-bar .mui-icon {
	color: #333;
	font-size: 35px;
	margin: 5px 0;
	
}
.search .mui-bar .search_aside{
	width: 85%;
	margin: 5px 0;
	padding: 5px 0;
	background-color: #EAEAEA;
	border-radius: 5px;
	display: inline-block;
}
.search .mui-bar .search_aside .span1{
	background: url(../images/search_icon1.png) no-repeat;
	display: inline-block;
	width: 15px;
	height: 15px;
	margin: 4px 0 0 10px;
	vertical-align: top;
}
.search .mui-bar .search_aside .input1{
	border: none;
	background-color: #EAEAEA;
	font-size: 14px;
	width: 85%;
}
.search .mui-bar .a1{
	float: right;
	color: #000000;
	padding: 11px 0;
	vertical-align: middle;
}
