	left: 0;
	height: 11px;
	width: 11px;
	background: url(../images/icon/logistics_address_gray.png) no-repeat center;
	z-index: 100;
}
.kf_logistics_address_list li:first-child:after{
	content:"";
	position: absolute;
	top:7px;
	left: 0;
	height: 11px;
	width: 11px;
	background: url(../images/icon/logistics_address.png) no-repeat center;
}
.kf_logistics_address_list li:before{
	content: "";
	position: absolute;
	top:-13px;
	/*bottom: 0;*/
	left: 5px;
	width: 1px;
	height: 100%;
	background:#d0d0d0;
	z-index: 2;
}
.kf_logistics_address_list li:first-child:before{
	height: 0;
}
.kf_logistics_address_list li span{
	display: block;
	line-height: 20px;
	font-size: 14px;
}
.kf_logistics_address_list li:first-child span{
	color:#e62929;
}
.kf_logistics_address_time{
	width: 145px;
	float: left;
}
.kf_logistics_address_content{
	margin-left: 145px;
}
.kf_logistics_goods{
	margin: 0 5px;
	float: left;
}
.kf_logistics_goods:first-child{
	margin-left: 0;
}
.kf_logistics_goods img{
	width: 60px;
	height: 60px;
}
.kf_logistics_goods label{
	display: block;
	text-align: center;
	margin-top: 5px;
}
.kf_mode_logistics .kf_group_title,
.kf_mode_logistics .kf_group_content{
	color:#37474f;
}



/*查看消息*/
.kf_footer{
	width: 365px;
	position: fixed;
	bottom: 20px;
	right: 20px;
	padding: 0 10px;
}
.kf_footer_news{
	width: 345px;
	height: 40px;
	background-color: #FFFFFF;
	border-radius: 2px;
	line-height: 40px;
	float: right;
	font-size: 12px;
	padding: 0 10px;
	border-bottom: 1px solid #e5e5e5;
	
}
.kf_footer_news:after{
	display: block;
	content: "";
	clear: both;
	visibility: hidden;
}
.kf_footer_news span{
	background: url(../images/home_email.png) no-repeat 100% 100%;
	width: 18px;
	height: 14px;
	display: inline-block;
	margin: 0 10px 0 0;
	
}
.kf_footer_news p{
	display: inline-block;
	/*position: relative;
	top: -4px;*/
}
.kf_footer_news p i{
	font-style: normal;
	color: #cd2520;
}
.kf_footer_news img{
	width: 30px;
	height: 30px;
	text-align: right;
	position: relative;
	top: 5px;
	float: right;
	cursor: pointer;
}
.kf_footer .kf_footer_ul{
	display: none;
	background-color: #FFFFFF;
	padding: 0 10px 0 10px;
}
.kf_footer .kf_footer_ul .kf_footer_li{
	padding: 5px 0;
	
}
.kf_footer .kf_footer_ul .kf_footer_li i{
	background: url(../images/home_alwaysemail.png) no-repeat 100% 100%;
	width: 14px;
	height: 10px;
	display: inline-block;
}
.kf_footer_li_active{
	background: url(../images/home_notemail.png) !important;
	height: 14px !important;
}
.kf_footer .kf_footer_ul .kf_footer_li p{
	display: inline-block;
	font-size: 12px;
	width: 330px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	margin: -3px 0;
	
}
.kf_footer .kf_footer_ul .kf_footer_li2{
	font-size: 12px;
	text-align: center;
	padding: 10px 0;
	
}
.kf_footer .kf_footer_ul .kf_footer_li2 i{
	background: url(../images/home_loadmore.png) no-repeat 100% 100%;
	width: 10px;
	height: 10px;
	display: inline-block;
	margin: 0 5px;
	
}
.kf_footer .kf_footer_ul .kf_footer_li2 span{
	color: #087DD1;
	cursor: pointer;
}
