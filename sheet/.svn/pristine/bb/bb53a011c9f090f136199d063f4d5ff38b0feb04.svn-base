/**
 * 引导条 
 */
.kf_workOrder_info_type{
	padding: 10px 0;
	overflow: hidden;
	zoom:1;
}
.kf_workOrder_info_type li{
	position: relative;
	height: 100px;
	width: 190px;
	float: left;
}
.kf_workOrder_info_type li:first-child{
	width: 90px;
}
.kf_workOrder_info_type li:before{
	content:"";
	position: absolute;
	top:50px;
	height: 1px;
	width: 120px;
	border-bottom: 1px dashed #ccc;
}
.kf_workOrder_info_type li:first-child:before{
	height: 0;
	width: 0;
	border-bottom: 0;
}
.kf_workOrder_info_type li:after{
	content:"";
	position: absolute;
	top:38px;
	right: 20px;
	display: block;
	height: 25px;
	width: 25px;
	background: url(../images/icon/list_img_n.png) no-repeat center;
}
.kf_workOrder_info_type li.active:after{
	background: url(../images/icon/list_img.png) no-repeat center;
	
}
.kf_workOrder_info_type span{
	position: absolute;
	text-align: center;
}
.kf_workOrder_info_type .active .kf_workOrder_info_type_name{
	color:#087dd1;
}
.kf_workOrder_info_type_name{
	width: 70px;
	top:10px;
	right: -2px;
	font-size: 16px;
}
.kf_workOrder_info_type_time{
	width: 90px;
	bottom: 12px;
	right: -12px;
}


