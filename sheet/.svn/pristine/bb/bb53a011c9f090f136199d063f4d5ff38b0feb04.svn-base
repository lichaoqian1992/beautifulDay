/*上传图片*/
.kf_upload_img{
	float: left;
	overflow: hidden;
	zoom:1;
}
.kf_form_result .kf_float_left.kf_group_text_tips {
	margin-left: 0;
}
.kf_upload_item{
	position: relative;
	height: 120px;
	width: 120px;
	margin-left: 20px;
	float: left;
}
.kf_upload_item:first-child{
	margin-left: 0;
}
.kf_uploat_item_close{
	position: absolute;
	top:0;
	right: 0;
	width:20px;
	height: 20px;
	background: url(../images/upload_close.png) no-repeat center;
	cursor: pointer;
}
.kf_upload_box{
	position: relative;
	display: block;
	height: 100%;
	width: 100%;
	background: url(../images/upload_bg.png) no-repeat center;
	cursor: pointer;
}
.kf_upload_item input[type='file']{
	position: absolute;
	top:0;
	left: 0;
	height: 100%;
	width: 100%;
	opacity: 0;
	filter:alpha(opacity=0);  
}
.kf_upload_box img{
	height: 100%;
	width: 100%;
}
.kf_form_footer{
	padding: 20px;
}
.kf_form_box .kf_form_footer{
	padding: 20px 0;
}
.kf_form_footer .kf_btn{
	margin-right: 10px;
}


.kf_select_change .kf_group{
	display: none;
}
.kf_select_change .kf_group.active{
