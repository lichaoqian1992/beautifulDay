/*
 * 单选按钮
 */
.kf_radio,
.kf_checkbox{
	position: relative;
	display: inline-block;
	height: 20px;
	line-height: 20px;
	padding-left: 30px;
	margin-right: 30px;
	cursor: pointer;
}
.kf_radio{
	background: url(../images/icon/radio_no.png) no-repeat left center;
}
.kf_radio.active{
	background: url(../images/icon/radio_yes.png) no-repeat left center;
}
.kf_checkbox{
	background: url(../images/icon/checkbox_main.png) no-repeat left center;
}
.kf_checkbox.active{
	background: url(../images/icon/login_state2.png) no-repeat left center;
}
.kf_checkbox_orange {
	background: url(../images/icon/checkbox_orange.png) no-repeat left center;
}
.kf_checkbox_orange.active{
	background: url(../images/icon/checkbox_ok_orange.png) no-repeat left center;
}
.kf_radio input[type="radio"],
.kf_checkbox input[type="checkbox"]{
	position: absolute;
	top:0;
	left: 0;
	opacity: 0;
	filter:alpha(opacity=0);  
}

