	<script>

	$(document).ready(function() {    

		    App.init(); 
		    infoset();
		    menuact("05_01_02");
		});
	
	function infoset(){
		
		var rolemedal=${rolemedal };
		
		var id=rolemedal.id;
		
		var	user_id=rolemedal.user_id;
		var	role_type=rolemedal.role_type;
		var	role_value=rolemedal.role_value;
		var	medal_value=rolemedal.medal_value;
		var	add_time=rolemedal.add_time;
		var	expired_time=rolemedal.expired_time;
		var	state=rolemedal.state;
		var	update_time=rolemedal.update_time;
		var	remark=rolemedal.remark;
		var	is_del=rolemedal.is_del;

	 	$("#hiddenid").val(id);
	 	$("#user_id").val(user_id);
	 	$("#role_type").val(role_type);
	 	$("#role_value").val(role_value);
	 	$("#medal_value").val(medal_value);
	 	$("#add_time").val(add_time);
	 	$("#expired_time").val(expired_time);
	 	$("#state").val(state);
	 	$("#update_time").val(update_time);
	 	$("#remark").val(remark);
	 	$("#is_del").val(is_del);

	};
	
	
	$("#subbutton").on("click",function(){

		$.ajax({
		type : "GET",
		url : "updUserRoleMedal",
		dataType : "json",
		contentType : "application/json; charset=utf-8",
		data : {
			id:$("#hiddenid").val(),
			user_id:$("#user_id").val(),
			role_type:$("#role_type").val(),
			role_value:$("#role_value").val(),
			medal_value:$("#medal_value").val(),
			add_time:$("#add_time").val(),
			expired_time:$("#expired_time").val(),
			state:$("#state").val(),
			update_time:$("#update_time").val(),
			remark:$("#remark").val(),
			is_del:$("#is_del").val()
				},
		success : function(data) {
			if(data.status==0){
				location.reload();
			}else{
				alert("修改失败");
			}
		}
				
		})
	});
	
	

	</script>

