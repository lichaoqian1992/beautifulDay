<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manji.backstage.mapper.CodeMapper" >

<select id="getCodeByName" parameterType="java.lang.String" resultType="SimpleCode">

	select * from bac_simplecode where name=#{0}
 
</select>

<update id="updCodeByName" parameterType="SimpleCode">

	update bac_simplecode
	set
	val=#{val}
	where id=#{id}
</update>



</mapper>