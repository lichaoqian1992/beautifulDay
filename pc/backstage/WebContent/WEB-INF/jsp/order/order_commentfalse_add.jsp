                            <input id="review_score" type="text"  class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">服务评分：</label>
                          <div class="controls">
                            <input id="service_review_score" type="text"  class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">包装评分：</label>
                          <div class="controls">
                            <input id="pack_review_score" type="text"   class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">物流评分：</label>
                          <div class="controls">
                            <input id="distribution_review_score" type="text"   class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">评价内容：</label>
                          <div class="controls">
                            <input id="comment_content" type="text"   class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">晒图地址集合：</label>
                          <div class="controls">
                            <input id="show_photos" type="text"   class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">是否收藏：</label>
                          <div class="controls">
                            <select id="is_house">
                              <option value="0">不收藏</option>
                              <option value="1">收藏</option>
                            </select>
                          </div>
                        </div>
                        <div class="control-group">
                          <label class="control-label">评价时间：</label>
                          <div class="controls">
                            <input id="comment_time" type="text"  class="m-wrap small" /><!--  <span class="help-inline">Some hint here</span> -->
